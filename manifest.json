{
  "name": "Banana For Scale",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "",
  "icons": {
    "16": "icons/banana.png",
    "48": "icons/banana.png",
    "128": "icons/banana.png"
  },
  "permissions": [
    "*"
  ],
  "content_scripts": [
    {
			"matches": ["<all_urls>"],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ],
  "web_accessible_resources": ["src/images/*.png"]
}